data/Historical_Data/*.csv
data/EOD_Data/*.csv

# Keep folder structure
!data/Historical_Data/.gitkeep
!data/EOD_Data/.gitkeep

# Python cache files
__pycache__/
*.py[cod]
*$py.class
*.so

# Virtual environments
.env
.venv
env/
venv/
ENV/

# OS specific files
.DS_Store
Thumbs.db
.DS_Store?
._*

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# Log files
*.log
logs/

# Temporary files
*.tmp
*.temp
temp/
tmp/

# Testing files
.pytest_cache/
.coverage
htmlcov/

# Old Google Drive credentials (no longer needed)
credentials.json
token.json